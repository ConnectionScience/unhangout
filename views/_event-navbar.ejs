    <% if(user.isAdminOf(event)) {%>
        <div class="admin-header">
        </div>
    <% } %>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="brand" href="/#"><%= event.get("title") %></a>
                <div class="nav-collapse collapse">
                    <ul class="nav">
                        <li class="" id="about-nav">
                        <a href="#about-nav">About<span class="updated circle hide">&nbsp;</span></a>
                        </li> 
                    </ul>

                    <div class="nav pull-left">
                        <div id="admin-region"></div>
                    </div>

                    <div class="nav pull-right identity">
                    <% if(!user) {%>
                    <a class="btn btn-primary pull-left" href="/auth/google">Login</a>
                    <% } else { %>
                        <div class="dropdown">
                            <a class="dropdown-toggle username" data-toggle="dropdown">
                            <img class="profile-thumbnail" src="<%= user.get("picture")%>" width="30px" height="30px">
                            <%= user.get("displayName")%> <i class="fa fa-caret-down"></i>&nbsp; &nbsp;
                            </a>

                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <% if(user.isAdminOf(event)) {%>
                                    <li role="presentation">
                                        <a role="menuitem" tabindex="-1" href="/admin">My Events</a>
                                    </li>
                                <% } %>

                                <li role="presentation">
                                    <a role="menuitem" tabindex="-1" href="/logout">Logout</a>
                                </li>
                            </ul>
                        </div>
                    <% } %>
                    
                    </div>
                </div><!--/.nav-collapse -->
            </div>
        </div>
    </div>
